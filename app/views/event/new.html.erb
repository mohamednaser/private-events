<h1>Event#new</h1>

<%= form_with url: events_new_path , model: @event do |f| %>
    <div class="form-group">
        <%= f.label :title, "title" %>
        <%= f.text_field :title %>
    </div>
    <div class="form-group">
        <%= f.label :description, "description" %>
        <%= f.text_field :description %>
    </div>

    <%= f.submit "Save Event"%>
<% end %>

